{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:8080/index.php",
        "http://localhost:8080/heroes.php",
        "http://localhost:8080/albums.php",
        "http://localhost:8080/comics.php",
        "http://localhost:8080/movies.php"
      ],
      "numberOfRuns": 1,
      "settings": {
        "chromeFlags": "--no-sandbox --disable-dev-shm-usage --ignore-certificate-errors --disable-features=TranslateUI"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    },
    "assert": {
      "assertions": {
        "categories:pwa": "off"
      }
    }
  }
}
